@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h2>Hakkımızda Veri Listesi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkında Başlığı</th>
        <th>Aktif Yapma</th>
        <th>Pasif Yapma</th>

    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <th>@x.AboutID</th>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-danger">Aktif Yap</a></td>
            <td><a href="#" class="btn btn-warning">Pasif Yap</a></td>

        </tr>
    }

</table>
@Html.Action("AboutPartial", "About")


